package practice;
public class gfhf{//以兩層迴圈印出俄羅斯方塊向右的圖形 

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//掌握何時印何時不印
		for(int i=0;i<30;i++) {
			for(int j=0;j<30;j++) {
				if(j<15||i>9&&i<20) {
					//為何要j小於15 因為15後不印
					//i>9但j是16要不要印？ 要
					System.out.printf("* ");
				}
			
			}
			System.out.printf("\n");
		}
	}

}
